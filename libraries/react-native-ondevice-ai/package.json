{
  "name": "react-native-ondevice-ai",
  "version": "0.1.0",
  "description": "React Native bindings for Locanara on-device AI SDK using Nitro Modules",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js",
      "react-native": "./src/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "nitrogen",
    "nitro.json",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "prepare": "bob build && npx nitrogen",
    "nitrogen": "nitrogen",
    "specs": "nitrogen --logLevel=\"debug\"",
    "lint:tsc": "tsc -p tsconfig.json --noEmit",
    "lint:eslint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:prettier": "prettier --write \"**/*.{md,js,jsx,ts,tsx}\"",
    "lint:ci": "bun run lint:tsc",
    "clean": "rm -rf android/build lib nitrogen/generated",
    "test": "jest --coverage",
    "example:android": "cd example && react-native run-android",
    "example:ios": "cd example && react-native run-ios",
    "example:start": "cd example && react-native start"
  },
  "keywords": [
    "react-native",
    "nitro",
    "on-device-ai",
    "locanara",
    "apple-intelligence",
    "gemini-nano"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyodotdev/locanara.git",
    "directory": "libraries/react-native-ondevice-ai"
  },
  "author": "hyodotdev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hyodotdev/locanara/issues"
  },
  "homepage": "https://github.com/hyodotdev/locanara/tree/main/libraries/react-native-ondevice-ai",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.0",
    "jest": "^29.7.0",
    "nitrogen": "^0.31.1",
    "prettier": "^3.3.3",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.38.4",
    "react-native-nitro-modules": "^0.31.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "^0.31.1"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "custom",
        {
          "script": "nitrogen",
          "clean": "nitrogen/"
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
