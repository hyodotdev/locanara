{
  "name": "@hyodotdev/locanara-gql",
  "version": "1.0.1",
  "type": "module",
  "main": "src/generated/types.ts",
  "exports": {
    ".": "./src/generated/types.ts",
    "./swift": "./src/generated/Types.swift",
    "./kotlin": "./src/generated/Types.kt",
    "./dart": "./src/generated/types.dart"
  },
  "scripts": {
    "generate": "bun run generate:ts && bun run codegen && bun run sync",
    "generate:ts": "graphql-codegen --config codegen.ts",
    "codegen": "bun codegen/cli.ts --all",
    "codegen:kotlin": "bun codegen/cli.ts --kotlin",
    "codegen:swift": "bun codegen/cli.ts --swift",
    "codegen:dart": "bun codegen/cli.ts --dart",
    "sync": "bun scripts/sync-to-platforms.mjs"
  },
  "keywords": [
    "graphql",
    "types",
    "locanara",
    "ai",
    "on-device"
  ],
  "author": "hyodotdev",
  "license": "AGPL-3.0",
  "description": "GraphQL schema and autogenerated types for Locanara",
  "devDependencies": {
    "@graphql-codegen/add": "^6.0.0",
    "@graphql-codegen/cli": "^6.0.0",
    "@graphql-codegen/typescript": "^5.0.0",
    "@types/node": "^22.0.0",
    "graphql": "^16.11.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "packageManager": "bun@1.1.0"
}
