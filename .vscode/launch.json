{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ Open Example App in Xcode",
      "command": "open Example/LocanaraExample.xcodeproj",
      "cwd": "${workspaceFolder}/packages/apple"
    },
    {
      "name": "ğŸ¤– Open Gemini (Android) in Android Studio",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "bash",
      "runtimeArgs": ["-lc", "./scripts/open-android-studio.sh"],
      "cwd": "${workspaceFolder}/packages/android",
      "console": "integratedTerminal"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“ GQL: Generate Types",
      "command": "bun run generate",
      "cwd": "${workspaceFolder}/packages/gql"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸŒ Run Site",
      "command": "bunx convex dev & bun run dev",
      "cwd": "${workspaceFolder}/packages/site"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± Expo: Web",
      "command": "bunx expo start --web",
      "cwd": "${workspaceFolder}/libraries/expo-ondevice-ai/example"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± Expo: iOS",
      "command": "bun ios --device",
      "cwd": "${workspaceFolder}/libraries/expo-ondevice-ai/example"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± Expo: Android",
      "command": "bun android",
      "cwd": "${workspaceFolder}/libraries/expo-ondevice-ai/example"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± React Native: Start",
      "command": "bun start",
      "cwd": "${workspaceFolder}/libraries/react-native-ondevice-ai/example"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± React Native: iOS",
      "command": "bun ios",
      "cwd": "${workspaceFolder}/libraries/react-native-ondevice-ai/example"
    },
    {
      "type": "node-terminal",
      "request": "launch",
      "name": "ğŸ“± React Native: Android",
      "command": "bun android",
      "cwd": "${workspaceFolder}/libraries/react-native-ondevice-ai/example"
    }
  ]
}
